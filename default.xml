<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="lirios" alias="origin" fetch="."/>

  <default revision="develop" remote="lirios" sync-j="4"/>

  <project name="repotools" path="misc/repotools" groups="misc"/>
  <project name="lirios" path="misc/manifest" groups="misc">
    <copyfile src="README.md" dest="README.md"/>
    <copyfile src="config/_clang-format" dest=".clang-format"/>
    <copyfile src="config/_commit-template" dest=".commit-template"/>
    <copyfile src="config/liri.pro" dest="liri.pro"/>
    <copyfile src="config/liri.qbs" dest="liri.qbs"/>
  </project>
  <project name="qmake-features" path="misc/qmake-features" groups="misc"/>
  <project name="qbs-shared" path="misc/qbs-shared" groups="misc"/>

  <project name="platformtheme" path="platformtheme" groups="desktop"/>
  <project name="wayland" path="wayland" groups="desktop"/>
  <project name="shell" path="shell" groups="desktop"/>
  <project name="workspace" path="workspace" groups="desktop"/>
  <project name="wallpapers" path="wallpapers" groups="desktop"/>

  <project name="fluid" path="fluid" groups="libraries"/>
  <project name="qtaccountsservice" path="qtaccountsservice" groups="libraries"/>
  <project name="qtgsettings" path="qtgsettings" groups="libraries"/>
  <project name="libliri" path="libliri" groups="libraries"/>
  <project name="vibe" path="vibe" groups="libraries"/>

  <project name="browser" path="liri-browser" groups="apps"/>
  <project name="calculator" path="liri-calculator" groups="apps"/>
  <project name="text" path="liri-text" groups="apps"/>
  <project name="appcenter" path="appcenter" groups="apps"/>
  <project name="settings" path="settings" groups="apps"/>
<!--
  <project name="terminal" path="terminal" groups="apps"/>
-->
  <project name="files" path="files" groups="apps"/>
</manifest>
