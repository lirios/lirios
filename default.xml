<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="lirios" alias="origin" fetch="."/>

  <default revision="develop" remote="lirios" sync-j="8" sync-s="true"/>

  <project name="repotools" path="misc/repotools" groups="misc"/>
  <project name="lirios" path="misc/manifest" groups="misc">
    <copyfile src="README.md" dest="README.md"/>
    <copyfile src="config/_clang-format" dest=".clang-format"/>
    <copyfile src="config/_commit-template" dest=".commit-template"/>
    <copyfile src="config/CMakeLists.txt" dest="CMakeLists.txt"/>
    <copyfile src="config/dependencies.cmake" dest=".dependencies.cmake"/>
    <copyfile src="config/LiriSubprojects.cmake" dest="cmake/LiriSubprojects.cmake"/>
    <copyfile src="env-setup.sh" dest="env-setup.sh"/>
    <copyfile src="format" dest="format"/>
    <copyfile src="monthly-log" dest="monthly-log"/>
  </project>

  <project name="eglfs" path="eglfs" groups="desktop"/>
  <project name="initial-setup" path="initial-setup" groups="desktop"/>
  <project name="libliri" path="libliri" groups="desktop"/>
  <project name="networkmanager" path="networkmanager" groups="desktop"/>
  <project name="qtintegration" path="qtintegration" groups="desktop"/>
  <project name="power-manager" path="power-manager" groups="desktop"/>
  <project name="pulseaudio" path="pulseaudio" groups="desktop"/>
  <project name="session" path="session" groups="desktop"/>
  <project name="settings" path="settings" groups="desktop"/>
  <project name="screencast" path="screencast" groups="desktop"/>
  <project name="screenshot" path="screenshot" groups="desktop"/>
  <project name="shell" path="shell" groups="desktop"/>
  <project name="themes" path="themes" groups="desktop"/>
  <project name="wallpapers" path="wallpapers" groups="desktop"/>
  <project name="qml-xwayland" path="qml-xwayland" groups="desktop"/>
  <project name="wayland" path="wayland" groups="desktop"/>
  <project name="xdg-desktop-portal-liri" path="xdg-desktop-portal-liri" groups="desktop"/>

  <project name="cmake-shared" path="cmake-shared" groups="common"/>
  <project name="qbs-shared" path="qbs-shared" groups="common"/>

  <project name="fluid" path="fluid" groups="libraries"/>
  <project name="qtaccountsservice" path="qtaccountsservice" groups="libraries"/>
  <project name="qtgsettings" path="qtgsettings" groups="libraries"/>
  <project name="qtudev" path="qtudev" groups="libraries"/>

  <project name="appcenter" path="appcenter" groups="apps"/>
  <project name="browser" path="browser" groups="apps"/>
  <project name="calculator" path="calculator" groups="apps"/>
  <project name="files" path="files" groups="apps"/>
  <project name="player" path="player" groups="apps"/>
  <project name="terminal" path="terminal" groups="apps"/>
  <project name="text" path="text" groups="apps"/>

  <project name="calamares-branding" path="calamares-branding" groups="os"/>
</manifest>
